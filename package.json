{
  "name": "@zhyporium/xendit",
  "version": "0.0.3",
  "description": "A type-safe Xendit API wrapper for TypeScript that provides full type checking for the Xendit API routes and responses.",
  "keywords": [],
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/zhyporium/xendit#readme",
  "bugs": {
    "url": "https://github.com/zhyporium/xendit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhyporium/xendit.git"
  },
  "author": "AOME <enum314@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "v:patch": "npm version patch -m \"chore(release): %s\" --sign-git-tag",
    "v:minor": "npm version minor -m \"chore(release): %s\" --sign-git-tag",
    "v:major": "npm version major -m \"chore(release): %s\" --sign-git-tag"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.1",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "dependencies": {
    "@zhyporium/rest": "^1.0.1"
  }
}
